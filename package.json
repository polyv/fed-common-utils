{
  "name": "@polyv/utils",
  "version": "2.3.0-beta.1",
  "description": "Utility functions of Polyv frontend development.",
  "keywords": [
    "polyv",
    "utility",
    "保利威"
  ],
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config build/webpack.dev.config.js",
    "gen-doc": "rimraf ./docs/2.x && typedoc --options ./typedoc.js --tsconfig ./src/tsconfig.json",
    "lint": "eslint ./src/*.ts",
    "compress": "node ./build/compress-js",
    "build": "npm run lint && rimraf ./dist/** && tsc -p ./src/tsconfig.json && tsc -p ./src/tsconfig.es5.json && npm run compress"
  },
  "files": [
    "dist/**/*"
  ],
  "miniprogram": "./",
  "main": "./dist/es/index.js",
  "types": "./dist/es/index.d.ts",
  "author": "Polyv frontend team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/polyv/fed-common-utils"
  },
  "bugs": {
    "url": "https://github.com/polyv/fed-common-utils/issues"
  },
  "homepage": "https://polyv.github.io/fed-common-utils/index.html",
  "engines": {
    "node": ">=10.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/plugin-proposal-class-properties": "^7.16.0",
    "@babel/plugin-proposal-json-strings": "^7.16.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-import-meta": "^7.10.4",
    "@babel/preset-env": "^7.16.4",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.3",
    "copy-webpack-plugin": "^6.4.1",
    "core-js": "^3.19.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.3.0",
    "eslint-plugin-sonarjs": "^0.11.0",
    "eslint-webpack-plugin": "^3.1.1",
    "glob": "^7.2.0",
    "html-webpack-plugin": "^4.5.2",
    "regenerator-runtime": "^0.13.9",
    "rimraf": "^3.0.2",
    "terser": "^5.10.0",
    "ts-loader": "^8.3.0",
    "typedoc": "^0.22.10",
    "typescript": "^4.5.2",
    "webpack": "^4.46.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.3",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {}
}
